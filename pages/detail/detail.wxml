<view class="page" data-weui-theme="{{theme}}">
  <view class="weui-form">
    <view class="weui-form__text-area">
      <view class="page__title page_align">{{tsName}}</view>
      <view class="page__desc page_align">{{name}}</view>
    </view>
    <view class="weui-form__control-area">
      <view class="weui-form-preview">
        <view class="weui-form-preview__hd">
          <view class="weui-form-preview__item">
            <em class="weui-form-preview__value">{{tsCode}}</em>
          </view>
        </view>
        <view class="weui-form-preview__bd">
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">净量</label>
            <text class="weui-form-preview__value">{{netBuy}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">15天量</label>
            <text class="weui-form-preview__value">{{count15}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">二到三</label>
            <text class="weui-form-preview__value">{{win15To23}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">二到四</label>
            <text class="weui-form-preview__value">{{win15To24}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">30天量</label>
            <text class="weui-form-preview__value">{{count30}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">二到三</label>
            <text class="weui-form-preview__value">{{win30To23}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">二到四</label>
            <text class="weui-form-preview__value">{{win30To24}}</text>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>