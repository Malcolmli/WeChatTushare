<view class="page" data-weui-theme="{{theme}}">
  <view class="weui-form">
    <view class="weui-form__text-area">
      <view class="page__title page_align">{{code}}-{{name}}</view>
    </view>
    <view class="weui-form__control-area">
      <view class="weui-form-preview">
        <view class="weui-form-preview__bd">
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">收益</label>
            <text class="weui-form-preview__value">{{profits}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">开始</label>
            <text class="weui-form-preview__value">{{start}}</text>
          </view>
          <view class="weui-form-preview__item">
            <label class="weui-form-preview__label">结束</label>
            <text class="weui-form-preview__value">{{end}}</text>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>