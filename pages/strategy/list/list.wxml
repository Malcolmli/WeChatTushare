<view class="page" data-weui-theme="{{theme}}">
  <view class="weui-form">
    <view class="page__bd page__bd_spacing">
      <view class="weui-flex">
        <view class="weui-flex__item">
          <view class="placeholder">{{query}}</view>
        </view>
      </view>
      <view class="weui-flex">
        <view class="weui-flex__item">
          <view class="placeholder">{{query1}}å¤©</view>
        </view>
        <view class="weui-flex__item">
          <view class="placeholder">{{query2}}%</view>
        </view>
        <view class="weui-flex__item">
          <view class="placeholder">{{query3}}%</view>
        </view>
        <view class="weui-flex__item">
          <view class="placeholder">{{query4}}%</view>
        </view>
      </view>
      <view class="weui-flex">
        <view class="weui-flex__item">
          <view class="placeholder">{{winRate}}</view>
        </view>
        <view class="weui-flex__item">
          <view class="placeholder">{{annualYield}}</view>
        </view>
      </view>
      <view class="weui-flex">
        <view class="weui-flex__item">
          <view class="placeholder">{{select}}</view>
        </view>
      </view>
    </view>
    <view class="weui-cells__group weui-cells__group_form">
      <view wx:for="{{array}}" wx:key="index">
        <navigator
          url="../detail/detail?name={{item.stock_name}}&code={{item.stock_code}}&start={{item.start_date}}&end={{item.end_date}}&profits={{item.profits}}&startPrice={{item.start_price}}&endPrice={{item.end_price}}&tradeCalSum={{item.tradeCalSum}}"
          class="weui-cell weui-cell_access" hover-class="weui-cell_active">
          <view class="weui-cell__bd">{{item.stock_name}}</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">{{item.profits}}</view>
        </navigator>
      </view>
    </view>
  </view>
</view>