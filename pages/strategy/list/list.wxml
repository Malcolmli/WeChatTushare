<view class="page" data-weui-theme="{{theme}}">
  <view class="weui-form">
    <view class="weui-form__text-area">
      <view class="weui-form__desc">{{info1}}ï¼Œ{{info2}}</view>
      <view class="weui-form__desc">{{select}}</view>
    </view>
    <view class="weui-form__control-area">
      <view class="weui-cells__group weui-cells__group_form">
        <view wx:for="{{array}}" wx:key="index">
          <navigator
          url="../detail/detail?name={{item.stock_name}}&code={{item.stock_code}}&start={{item.start_date}}&end={{item.end_date}}&profits={{item.profits}}&startPrice={{item.start_price}}&endPrice={{item.end_price}}"
            class="weui-cell weui-cell_access" hover-class="weui-cell_active">
            <view class="weui-cell__bd">{{item.stock_name}}</view>
            <view class="weui-cell__ft weui-cell__ft_in-access">{{item.profits}}</view>
          </navigator>
        </view>
      </view>
    </view>
  </view>
</view>